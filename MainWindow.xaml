<Window x:Class="CryplexAdmin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="450" Width="1080">
    <Grid>
        <StackPanel Height="35" VerticalAlignment="Top" Orientation="Horizontal" Margin="10,0,0,0">
            <Button VerticalAlignment="Center" HorizontalAlignment="Center" Width="150" Click="NavigateToProducts" Margin="0,0,11,0" >
                Products
            </Button>
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" Width="150" Click="NavigateToLicenses">
                Licenses
            </Button>
        </StackPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="0,40,0,0"/>
    </Grid>
</Window>
